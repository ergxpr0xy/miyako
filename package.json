    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "brunch": "^2.8.2",
    "clean-css-brunch": "^2.0.0",
    "css-brunch": "^2.6.1",
    "del-cli": "^0.2.0",
    "environment-brunch": "^1.0.0",
    "javascript-brunch": "^2.0.0",
    "less-brunch": "^2.7.1",
    "markdown-brunch": "^1.7.2",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
