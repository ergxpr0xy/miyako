    "auto-reload-brunch": "^2.7.1",
    "babel-brunch": "^6.0.6",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "bowser": "^1.4.6",
    "brunch": "^2.8.2",
    "clean-css-brunch": "^2.0.0",
    "css-brunch": "^2.6.1",
    "del-cli": "^0.2.0",
    "environment-brunch": "^1.0.0",
