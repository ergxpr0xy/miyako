{
  "name": "miyako",
  "version": "0.3.3",
  "description": "Simple app to download photos and photo albums",
  "private": true,
  "main": "server.js",
  "scripts": {
    "watch:server": "nodemon server.js --ignore public/",
    "watch:app": "brunch w",
    "watch": "run-p watch:*",
    "clean": "del-cli public/** !public",
    "build": "npm run clean && brunch b -p && del-cli public/test**",
    "start": "node server.js"
  },
