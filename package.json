{
  "name": "miyako",
  "version": "0.4.0",
  "description": "Photos and images downloader app",
  "private": true,
  "main": "server.js",
  "homepage": "http://tkesgar-miyako.herokuapp.com",
  "keywords": [
    "photo",
    "image",
    "download",
    "website"
  ],
  "scripts": {
    "watch:server": "nodemon server.js --ignore public/ --ignore app/",
    "watch:app": "brunch watch",
    "watch": "npm-run-all --parallel watch:*",
    "clean": "del-cli public/** !public",
    "build": "brunch build --production && del-cli public/test**",
    "all": "npm-run-all clean build"
  },
  "engines": {
    "node": ">=5.11.0",
    "npm": ">=3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkesgar/miyako.git"
  },
  "bugs": {
    "url": "https://github.com/tkesgar/miyako/issues",
    "email": "t.kesgar@live.com"
  },
  "author": "Ted Kesgar <t.kesgar@live.com> (https://tkesgar.com)",
  "contributors": [
    "Ted Kesgar <t.kesgar@live.com> (https://tkesgar.com)"
  ],
  "license": "MIT",
